[visual] # Visualizer filter

# Effects list (string)
#effect-list=spectrum

# Video width (geheel getal)
#effect-width=800

# Video height (geheel getal)
#effect-height=500

# FFT window (string)
#effect-fft-window=flat

# Kaiser window parameter (drijvendekommagetal)
#effect-kaiser-param=3.000000

# Show 80 bands instead of 20 (boolean)
#visual-80-bands=1

# Draw peaks in the analyzer (boolean)
#visual-peaks=1

# Enable original graphic spectrum (boolean)
#spect-show-original=0

# Draw the base of the bands (boolean)
#spect-show-base=1

# Base pixel radius (geheel getal)
#spect-radius=42

# Spectral sections (geheel getal)
#spect-sections=3

# V-plane color (geheel getal)
#spect-color=80

# Draw bands in the spectrometer (boolean)
#spect-show-bands=1

# Show 80 bands instead of 20 (boolean)
#spect-80-bands=1

# Number of blank pixels between bands. (geheel getal)
#spect-separ=1

# Amplification (geheel getal)
#spect-amp=8

# Draw peaks in the analyzer (boolean)
#spect-show-peaks=1

# Peak extra width (geheel getal)
#spect-peak-width=61

# Peak height (geheel getal)
#spect-peak-height=1

